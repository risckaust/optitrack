<launch>
	
	<group ns="defender1">
		<arg name="rb_id" value="0" />
		<param name="rigidbody_id" type="int" value="$(arg rb_id)" />
		<node name="mocap2mavros_rb_$(arg rb_id)" pkg="optitrack" type="mocap_pose.py" output="screen">
		</node>
	</group>

	<group ns="defender2">
		<arg name="rb_id" value="1" />
		<param name="rigidbody_id" type="int" value="$(arg rb_id)" />
		<node name="mocap2mavros_rb_$(arg rb_id)" pkg="optitrack" type="mocap_pose.py" output="screen">
		</node>
	</group>

	<group ns="defender3">
		<arg name="rb_id" value="2" />
		<param name="rigidbody_id" type="int" value="$(arg rb_id)" />
		<node name="mocap2mavros_rb_$(arg rb_id)" pkg="optitrack" type="mocap_pose.py" output="screen">
		</node>
	</group>

	<group ns="attacker1">
		<arg name="rb_id" value="3" />
		<param name="rigidbody_id" type="int" value="$(arg rb_id)" />
		<node name="mocap2mavros_rb_$(arg rb_id)" pkg="optitrack" type="mocap_pose.py" output="screen">
		</node>
	</group>

</launch>
